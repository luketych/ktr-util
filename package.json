{
  "name": "ktr-util",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "packageManager": "npm@9.6.4",
  "scripts": {
    "dev": "node --trace-warnings --async-stack-traces --experimental-specifier-resolution ./index.js",
    "start": "node --trace-warnings --async-stack-traces --experimental-specifier-resolution node ./index.js",
    "test": "mocha test --recursive --reporter spec --exit --timeout 10000"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "imports": {
    "#projectRoot": "./",
    "#projectRoot/*": "./*.js",
    "#src": "./src/index.js",
    "#src/*": "./src/*.js"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "date-holidays": "^3.16.11",
    "dotenv": "^16.0.3",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.8.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.1.0"
  }
}
